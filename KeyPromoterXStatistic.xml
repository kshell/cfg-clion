<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Data Source Properties">
        <Item>
          <IdeaActionID>DatabaseView.PropertiesAction</IdeaActionID>
          <ShortCut>'Shift+Enter'</ShortCut>
          <Description>Data Source Properties</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Duplicate">
        <Item>
          <IdeaActionID>DatabaseView.CopyAction</IdeaActionID>
          <ShortCut>'Ctrl+D'</ShortCut>
          <Description>Duplicate</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Execute">
        <Item>
          <IdeaActionID>Console.Jdbc.Execute</IdeaActionID>
          <ShortCut>'Ctrl+Enter'</ShortCut>
          <Description>Execute</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Remove">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'Delete'</ShortCut>
          <Description>Remove</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>